// FOOTER :: START

footer.footer
  .container-fluid
    .footer__wrapper

      .container-inner

        .footer__row
          .footer__col.footer__col-0.wow.wowFadeUp(data-animation-delay="0.3s")
            a.footer__email(
              href="mailto:sales@info-cat.ru"
              title=""
              class=""
            )!= "sales@info-cat.ru"

          .footer__col.footer__col-1.wow.wowFadeUp(data-animation-delay="0.4s")
            p.footer__address!= "Москва, ул. Енисейская д. 1, стр. 8, оф. 105"

          .footer__col.footer__col-2.wow.wowFadeUp(data-animation-delay="0.4s")
            p.footer__copyright!= "© 2018 infocat"

          .footer__col.footer__col-3.wow.wowFadeUp(data-animation-delay="0.3s")
            a(
              href="#"
              title=""
              class=""
              target="_blank"
            )
              p.footer__developBy
                != "Разработка сайта"
              i.icon-shogo-logo

// FOOTER :: END
