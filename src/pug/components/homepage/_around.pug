section.around
  .around__wrapper

    h1.around__title.title.wow.wowFadeUp(data-animation-delay="0.3s")!= "О компании"

    .swiper-container.around-slide-js
      .swiper-wrapper
        .swiper-slide
          .around__row
            .around__row-left.wow.wowFadeUp(data-animation-delay="0.4s")

              div.around__count(around-count-js)
                p!= "<span main-count-js></span> <span>/</span> <span main-len-js></span>"

              p.around__subtitle.wow.wowFadeUp(data-animation-delay="0.45s")!= "Лидер"

              p.with-dot.wow.wowFadeUp(data-animation-delay="0.5s")!= "Мы стремимся быть лидерами <br>в интеграции систем <br>хранения данных, <br>информационных технологий <br>и безопасности."

            .around__row-right.wow.wowFadeUp(data-animation-delay="0.55s")
              img.around__img(
                src="./images/img-bg__around.png"
                srcset="./images/img-bg__around@2x.png 2x"
                alt=""
                title=""
               )

        .swiper-slide
          .around__row
            .around__row-left

              div.around__count(around-count-js)
                p!= "<span main-count-js></span> <span>/</span> <span main-len-js></span>"

              p.around__subtitle!= "Лидер"

              p.with-dot!= "Мы стремимся быть лидерами <br>в интеграции систем <br>хранения данных, <br>информационных технологий <br>и безопасности."

